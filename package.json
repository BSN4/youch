{
  "name": "youch",
  "version": "2.0.9",
  "description": "HTML Pretty error stack viewer",
  "main": "src/Youch.js",
  "files": [
    "src"
  ],
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "node bin/compile.js",
    "test": "npm run build && japa",
    "test:win": "npm run build && node ./node_modules/japa-cli/index.js",
    "lint": "standard"
  },
  "author": "amanvirk",
  "license": "MIT",
  "devDependencies": {
    "concat": "^1.0.3",
    "cz-conventional-changelog": "^2.1.0",
    "japa": "^1.0.6",
    "japa-cli": "^1.0.1",
    "standard": "^11.0.1",
    "supertest": "^3.1.0",
    "uglify-js": "^3.4.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "cookie": "^0.3.1",
    "mustache": "^2.3.1",
    "stack-trace": "0.0.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poppinss/youch.git"
  },
  "keywords": [
    "errors",
    "error-reporting",
    "whoops"
  ],
  "bugs": {
    "url": "https://github.com/poppinss/youch/issues"
